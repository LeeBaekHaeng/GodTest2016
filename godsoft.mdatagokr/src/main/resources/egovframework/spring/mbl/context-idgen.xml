<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- 신규공통컴포넌트 IDGEN -->
	<!-- 실시간공지서비스 -->
	<bean name="egovRealtimeNoticeIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="strategy"   ref="realtimeNoticeStrategy" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="RNS_ID"/>
	</bean>
	<bean name="realtimeNoticeStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix"   value="RNS_" />
		<property name="cipers"   value="16" />
		<property name="fillChar" value="0" />
	</bean>
	
	<!-- 모바일 사진 앨범 -->
	<bean name="egovPhotoIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="strategy"   ref="photoStrategy" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="MPA_ID"/>
	</bean>
	<bean name="photoStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix"   value="MPA_" />
		<property name="cipers"   value="16" />
		<property name="fillChar" value="0" />
	</bean>	
	
	<!-- 모바일 차트/그래프 -->
	<bean name="egovChartGraphIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="strategy"   ref="chartGraphStrategy" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="MCG_ID"/>
	</bean>
	<bean name="chartGraphStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix"   value="MCG_" />
		<property name="cipers"   value="16" />
		<property name="fillChar" value="0" />
	</bean>
	
	<!-- 모바일 기기 식별 -->
	<bean name="egovDeviceIdentIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="strategy"   ref="deviceIdentStrategy" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="MDI_ID"/>
	</bean>
	<bean name="deviceIdentStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix"   value="MDI_" />
		<property name="cipers"   value="16" />
		<property name="fillChar" value="0" />
	</bean>
	
	<!-- 멀티미디어 제어 -->
	<bean name="egovMultimediaIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="strategy"   ref="multimediaStrategy" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="MLT_ID"/>
	</bean>
	<bean name="multimediaStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix"   value="MLT_" />
		<property name="cipers"   value="16" />
		<property name="fillChar" value="0" />
	</bean>
	
	<!-- OPEN-API 서비스 연계 -->
	<bean name="egovOpenApiIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="OAS_ID"/>
	</bean>
	
	<!-- 위치정보 연계 서비스 -->
	<bean name="egovGeoLocationIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="GEO_ID"/>
	</bean>
	
	<!-- MMS 서비스 연계 -->
	<bean name="egovMmsIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="MMS_ID"/>
	</bean>
	
	<!-- MMS 첨부파일 -->
	<bean name="egovAttachFileIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="egov.dataSource" />
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="COMTECOPSEQ"/>
		<property name="tableName"	value="ATCH_ID"/>
	</bean>
         
</beans>
